{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "target": "ESNext",
    "jsx": "react-jsx",
    "types": ["react", "react-dom", "node"]
  },
  "exclude": ["node_modules", "dist"]
}
