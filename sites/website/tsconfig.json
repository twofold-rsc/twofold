{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "types": ["react", "react-dom", "node", "@twofold/framework"],
    "paths": {
      "@/lib/*": ["./lib/*"],
      "@/public/*": ["./public/*"]
    }
  },
  "include": ["./app/", "./lib/", "./scripts/", "./public/"]
}
